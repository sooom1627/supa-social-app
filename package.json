    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
